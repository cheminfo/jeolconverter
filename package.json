{
  "name": "jeolconverter",
  "version": "0.1.3",
  "description": "read JEOL NMR files",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "rollup -c",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "test-only": "jest",
    "build": "cheminfo-build --entry src/index.js --root jeolconverter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/jeolconverter.git"
  },
  "keywords": [
    "magnetic resonance",
    "nmr",
    "jeol",
    "dataset",
    "converter",
    "parser"
  ],
  "author": "Julien Wist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/jeolconverter/issues"
  },
  "homepage": "https://github.com/cheminfo/jeolconverter#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@types/jest": "^25.2.2",
    "cheminfo-build": "^1.1.1",
    "cheminfo-tools": "^1.23.3",
    "eslint": "^7.0.0",
    "eslint-config-cheminfo": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "rollup": "^2.10.2",
    "jeol-data-test": "^0.2.3"
  },
  "dependencies": {
    "iobuffer": "^5.0.2"
  }
}
